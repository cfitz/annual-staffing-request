<% klass = archive? ? ArchivedStaffRequest : StaffRequest %>
<div class="page-header">
  <h1>Staff Requests</h1>
</div>

<%= render partial: 'shared/index_action_buttons',
  locals: { klass: klass, pagination_object: @staff_requests } %>
<%= render partial: 'shared/table', locals: { klass: klass,  records: @staff_requests } %>
<%= will_paginate renderer: BootstrapPagination::Rails %>
