<div class="page-header">
  <h1><%= @role_type.code %> <small>Role Type</small></h1>
</div>

<table class="table table-striped">
  <tbody>
    <tr>
      <th>Code:</th>
      <td id="code"><%= @role_type.code %></td>
    </tr>

    <tr>
      <th>Name:</th>
      <td id="name"><%= @role_type.name %></td>
    </tr>
  </tbody>
</table>

<%= render 'shared/show_action_buttons', object: @role_type %>
