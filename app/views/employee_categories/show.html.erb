<div class="page-header">
  <h1><%= @employee_category.code %> <small>Employee Category</small></h1>
</div>

<table class="table table-striped">
  <tbody>
    <tr>
      <th>Code:</th>
      <td id="code"><%= @employee_category.code %></td>
    </tr>

    <tr>
      <th>Name:</th>
      <td id="name"><%= @employee_category.name %></td>
    </tr>
  </tbody>
</table>

<%= render 'shared/show_action_buttons', object: @employee_category %>
