<thead class='<%= "#{klass.to_s.underscore}_fields" %>'>
  <tr>
    <th>
      <input id="select-all" type="checkbox"></input> 
    </th>
    <%- fields(klass).each do |field | %>
      <% f = %i( annual_cost hourly_rate  ).include?(field) ? "#{field.to_s}_cents".intern : field %> 
      <th id="<%= f.to_s %>"><%= multi_sort_link( @q, f, klass.human_attribute_name(field) ) %></th>
    <% end %>
    <%= render 'shared/table_action_buttons_header',  model_type: klass  %>
  </tr>
</thead>
