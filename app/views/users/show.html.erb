<div class='container'>
  <div class="page-header">
    <h1><%= @user.cas_directory_id %> <small>User</small></h1>
  </div>

  <table class="table table-striped">
    <tbody>
      <tr>
        <th>CAS Directory ID:</th>
        <td id="cas_directory_id"><%= @user.cas_directory_id %></td>
      </tr>

      <tr>
        <th>Name:</th>
        <td id="name"><%= @user.name %></td>
      </tr>
    </tbody>
  </table>

  <%= render 'shared/show_action_buttons', object: @user %>

</div>
